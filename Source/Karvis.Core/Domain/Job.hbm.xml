<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="Karvis.Core" assembly="Karvis.Core">

  <class name="Job">

    <id name="Id">
      <generator class="identity" />
    </id>

    <property name="Title"/>
    <property name="Description"/>
    <property name="VisitCount"/>
    <property name="FeedCount"/>
    <property name="Tag"/>
    <property name="DateAdded"/>
    <property name="Url"/>
  </class>

</hibernate-mapping>